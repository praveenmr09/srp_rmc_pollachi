<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="booking_wizard" model="ir.ui.view">
        <field name="name">booking.wizard.wizard</field>
        <field name="model">booking.wizard</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <div class="alert alert-success mb-4" role="alert">
                        Booking DashBoard
                    </div>
                    <group>
                        <group>
                            <field name="date"/>
                        </group>
                        <group>
                            <field name="vehicle"/>
                        </group>
                    </group>
                    <footer>
                        <button
                                name="tick_ok"
                                string="Collect"
                                type="object"
                                class="btn btn-success"
                        />
                        <button
                                string="Cancel"
                                icon="fa-close"
                                class="btn btn-danger"
                                special="cancel"
                        />
                    </footer>
                </sheet>
            </form>
        </field>
    </record>
</odoo>